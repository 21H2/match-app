{
  "version": "1.0.0",
  "name": "@bunk/mobile-frontend",
  "main": "expo-router/entry",
  "private": true,
  "scripts": {
    "start": "expo start",
    "start:tunnel": "expo start --tunnel",
    "start:lan": "expo start --lan",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "prebuild": "expo prebuild",
    "build:dev": "eas build --platform android --profile development",
    "build:preview": "eas build --platform android --profile preview",
    "build:prod": "eas build --platform android --profile production",
    "build:ios:prod": "eas build --platform ios --profile production",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios"
  },
  "dependencies": {
    "expo": "~54.0.0",
    "expo-constants": "~17.0.3",
    "expo-font": "~13.0.1",
    "expo-linking": "~7.0.4",
    "expo-router": "~4.0.15",
    "expo-splash-screen": "~0.29.19",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.5",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.4",
    "react-native-safe-area-context": "~4.12.0",
    "react-native-screens": "~4.3.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~18.3.12",
    "@types/react-dom": "~18.3.1",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.2",
    "prettier": "^3.4.2",
    "typescript": "~5.6.2"
  }
}
